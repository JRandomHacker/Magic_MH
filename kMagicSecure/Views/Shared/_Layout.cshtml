							<li>@Html.ActionLink("HOU(2)", "Index", new { controller = "Magic", action = "Index", eventName = "HOU", round = 2 })</li>
							<li>@Html.ActionLink("HOU(3)", "Index", new { controller = "Magic", action = "Index", eventName = "HOU", round = 3 })</li>
							<li>@Html.ActionLink("HOU(*)", "Index", new { controller = "Magic", action = "Index", eventName = "HOU", round = 0 })</li>
							<li>@Html.ActionLink("HOU(Top 8)", "Index", new { controller = "Magic", action = "Index", eventName = "HOU", round = 4 })</li>
