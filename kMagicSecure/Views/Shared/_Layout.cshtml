<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - kMagic Scores</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>
	@{
		try
		{
			Exception lastError = (Exception)Session["LastError"];
			if (lastError != null)
			{
				<div class="errorTitle">ERROR: @lastError.Message</div>
				Session["LastError"] = null;
			}
		}
		catch (Exception ex)
		{
			<div class="errorTitle">ERROR: Unknown Error.</div>
			Session["LastError"] = null;
		}
	}



	<div class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				@Html.ActionLink("Magic", "Index", new { controller = "Magic", action = "Default" }, new { @class = "navbar-brand" })
			</div>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<li class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle">MM3<b class="caret"></b></a>
						<ul class="dropdown-menu">
							<li>@Html.ActionLink("MM3(1)", "Index", new { controller = "Magic", action = "Index", eventName = "MM3", round = 1 })</li>
							<li>@Html.ActionLink("MM3(2)", "Index", new { controller = "Magic", action = "Index", eventName = "MM3", round = 2 })</li>
							<li>@Html.ActionLink("MM3(3)", "Index", new { controller = "Magic", action = "Index", eventName = "MM3", round = 3 })</li>
							<li>@Html.ActionLink("MM3(*)", "Index", new { controller = "Magic", action = "Index", eventName = "MM3", round = 0 })</li>
						</ul>
					</li>
					<li class="dropdown">
