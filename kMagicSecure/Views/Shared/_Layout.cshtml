<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - kMagic Scores</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>
	@{
		Exception lastError = (Exception)Session["LastError"];
		if (lastError != null)
		{
			<div class="errorTitle">ERROR: @lastError.Message</div>
			Session["LastError"] = null;
		}
	}



	<div class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				@Html.ActionLink("Magic", "Index", new { controller = "Magic", action = "Default"}, new { @class = "navbar-brand" })
			</div>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<li>@Html.ActionLink("RMZ", "Index", new { controller = "Magic", action = "Index", eventName = "RMZ", round = 1 })</li>
					<li class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle">OGW<b class="caret"></b></a>
						<ul class="dropdown-menu">
							<li>@Html.ActionLink("OGW(1)", "Index", new { controller = "Magic", action = "Index", eventName = "OGW", round = 1 })</li>
							<li>@Html.ActionLink("OGW(2)", "Index", new { controller = "Magic", action = "Index", eventName = "OGW", round = 2 })</li>

							<li>@Html.ActionLink("OGW(3)", "Index", new { controller = "Magic", action = "Index", eventName = "OGW", round = 3 })</li>
							<li>@Html.ActionLink("OGW(*)", "Index", new { controller = "Magic", action = "Index", eventName = "OGW", round = 0 })</li>
							<li>@Html.ActionLink("OGW(Top 8)", "Index", new { controller = "Magic", action = "Index", eventName = "OGW", round = 4 })</li>
						</ul>
					</li>
					<li class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle">SOI<b class="caret"></b></a>
						<ul class="dropdown-menu">
							<li>@Html.ActionLink("SOI(1)", "Index", new { controller = "Magic", action = "Index", eventName = "SOI", round = 1 })</li>
							<li>@Html.ActionLink("SOI(2)", "Index", new { controller = "Magic", action = "Index", eventName = "SOI", round = 2 })</li>
							<li>@Html.ActionLink("SOI(3)", "Index", new { controller = "Magic", action = "Index", eventName = "SOI", round = 3 })</li>
							<li>@Html.ActionLink("SOI(*)", "Index", new { controller = "Magic", action = "Index", eventName = "SOI", round = 0 })</li>
							<li>@Html.ActionLink("SOI(Top 8)", "Index", new { controller = "Magic", action = "Index", eventName = "SOI", round = 4 })</li>
						</ul>
					</li>
					<li class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle">EMN<b class="caret"></b></a>
						<ul class="dropdown-menu">
							<li>@Html.ActionLink("EMN(1)", "Index", new { controller = "Magic", action = "Index", eventName = "EMN", round = 1 })</li>
							<li>@Html.ActionLink("EMN(2)", "Index", new { controller = "Magic", action = "Index", eventName = "EMN", round = 2 })</li>
							<li>@Html.ActionLink("EMN(3)", "Index", new { controller = "Magic", action = "Index", eventName = "EMN", round = 3 })</li>
							<li>@Html.ActionLink("EMN(*)", "Index", new { controller = "Magic", action = "Index", eventName = "EMN", round = 0 })</li>
							<li>@Html.ActionLink("EMN(Top 8)", "Index", new { controller = "Magic", action = "Index", eventName = "EMN", round = 4 })</li>
						</ul>
					</li>
				</ul>
				@Html.Partial("_loginPartial")
			</div>
		</div>
	</div>
	<div class="container body-content">
		@RenderBody()
		<hr />
		<footer>
			<p>&copy; 2015-2016 - MHill kMagic Manager</p>
		</footer>
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)
</body>
</html>
