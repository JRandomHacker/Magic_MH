							<li>@Html.ActionLink("AKH(3)", "Index", new { controller = "Magic", action = "Index", eventName = "AKH", round = 3 })</li>
							<li>@Html.ActionLink("AKH(*)", "Index", new { controller = "Magic", action = "Index", eventName = "AKH", round = 0 })</li>
							<li>@Html.ActionLink("AKH(Top 8)", "Index", new { controller = "Magic", action = "Index", eventName = "AKH", round = 4 })</li>
						</ul>
					</li>
					<li class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle">HOU<b class="caret"></b></a>
						<ul class="dropdown-menu">
